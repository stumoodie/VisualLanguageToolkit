<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.pathwayeditor.businessobjects.hibernate.pojos.HibBendPoint" table="bendpoint">
        <id name="id" type="java.lang.Long">
            <column name="id" />
            <generator class="native" />
        </id>
        <properties name="xak1bendpoint" unique="true">
        	<many-to-one name="owningLink" class="org.pathwayeditor.businessobjects.hibernate.pojos.HibLink" fetch="select">
            	<column name="link_id" not-null="true" />
        	</many-to-one>
        	<property name="indexPos" type="int">
            	<column name="index_pos" not-null="true" />
        	</property>
        </properties>
        <property name="XPosition" type="int">
            <column name="x_position" not-null="true" />
        </property>
        <property name="YPosition" type="int">
            <column name="y_position" not-null="true" />
        </property>
    </class>
</hibernate-mapping>
