<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="org.pathwayeditor.businessobjects.hibernate.pojos.HibModel" table="model">
        <id name="id" type="java.lang.Long">
            <column name="id" />
            <generator class="native" />
        </id>
        	<many-to-one name="canvas" class="org.pathwayeditor.businessobjects.hibernate.pojos.HibCanvas">
            	<column name="canvas_id" not-null="true" />
        	</many-to-one>
        <many-to-one name="rootNode" class="org.pathwayeditor.businessobjects.hibernate.pojos.HibCompoundNode">
            <column name="root_node_id" not-null="false"/>
        </many-to-one>
    </class>
</hibernate-mapping>
